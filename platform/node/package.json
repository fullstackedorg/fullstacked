{
    "name": "fullstacked",
    "version": "0.12.3-1442",
    "scripts": {
        "build": "node build.js",
        "postbuild": "node build.js --binding",
        "start": "npm run build && node index.js --debug --lib ../../core/bin --root ~/FullStacked --config ~/.config/fullstacked --editor ../../out/build",
        "prepack": "node build.js --fullstacked_modules"
    },
    "bin": {
        "fullstacked": "index.js"
    },
    "type": "module",
    "dependencies": {
        "cli-progress": "^3.12.0",
        "jsdom": "^28.1.0",
        "open": "^11.0.0",
        "pretty-bytes": "^7.1.0",
        "sass": "^1.97.3",
        "tar-stream": "^3.1.8",
        "ws": "^8.19.0"
    },
    "devDependencies": {
        "@types/cli-progress": "^3.11.6",
        "@types/jsdom": "^28.0.0",
        "@types/tar-stream": "^3.1.4",
        "@types/ws": "^8.18.1",
        "node-addon-api": "^8.6.0"
    },
    "overrides": {
        "cli-progress": {
            "string-width": "^8.1.0"
        }
    },
    "author": "FullStacked",
    "license": "MIT",
    "description": "Create, run and share projects built with web technologies in a fully cross-platform, local-first environment.",
    "homepage": "https://fullstacked.org",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fullstackedorg/fullstacked.git"
    }
