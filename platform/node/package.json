{
    "name": "fullstacked",
    "version": "0.12.0-1175",
    "scripts": {
        "build": "node build.js",
        "postbuild": "node build.js --binding",
        "start": "npm run build && node index.js --lib ../../core/bin --root ~/FullStacked --config ~/.config/fullstacked --editor ../../out/build",
        "prepack": "npm run build"
    },
    "bin": {
        "fullstacked": "index.js"
    },
    "type": "module",
    "dependencies": {
        "cli-progress": "^3.12.0",
        "open": "^10.2.0",
        "pretty-bytes": "^7.1.0",
        "tar-stream": "^3.1.7",
        "ws": "^8.18.3"
    },
    "devDependencies": {
        "@types/cli-progress": "^3.11.6",
        "@types/tar-stream": "^3.1.4",
        "@types/ws": "^8.18.1",
        "node-addon-api": "^8.5.0"
    },
    "overrides": {
        "cli-progress": {
            "string-width": "^8.1.0"
        }
    },
    "author": "FullStacked",
    "license": "MIT",
    "description": "Create, run and share projects built with web technologies in a fully cross-platform, local-first environment.",
    "homepage": "https://fullstacked.org",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fullstackedorg/fullstacked.git"
    }
}
