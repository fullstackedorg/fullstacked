{
    "compilerOptions": {
        "esModuleInterop": true,
        "module": "esnext",
        "target": "esnext",
        "moduleResolution": "bundler",
        "skipLibCheck": true,
        "jsx": "react",
        "allowImportingTsExtensions": true,
        "noEmit": true,
        "paths": {
            "*": ["./fullstacked_modules/*"]
        }
    },
    "exclude": [
        "demo",
        "out",
        "core",
        "platform/android",
        "platform/apple",
        "platform/docker",
        "platform/linux",
        "platform/node/fullstacked_modules",
        "platform/windows"
    ]
}
